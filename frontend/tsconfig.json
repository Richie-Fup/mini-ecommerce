{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@tarojs/taro", "@tarojs/components"]
  },
  "include": ["src"]
}


